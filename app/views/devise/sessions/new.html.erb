


<div class="container text-center">

	<div class="row text-center"> 
	<h2>Log in</h2>
		<div class="cols-sm-4 card text-center">
			<div class="card-small-padding text-center">
				<ul class="list-inline">
					<li>
						<a href="#">
						<img src="http://res.cloudinary.com/caroheymes/image/upload/c_scale,h_20,w_20/v1510593712/turbotax_kx7bvv.png" alt=""> turbotax
						</a>
					</li>
					<li>
						<a href="#">
						<img src="http://res.cloudinary.com/caroheymes/image/upload/c_scale,h_20,w_20/v1510593635/quickbooks_c4mc91.png" alt=""> quickbooks
						</a>
					</li>
					<li>
						<a href="#">
						<img src="http://res.cloudinary.com/caroheymes/image/upload/c_scale,h_20/v1510594034/mint_ktbtd3.png" alt=""> mint
						</a>
					</li>
				</ul>
			</div>
					<!--Filet central-->
			<div class="border-top align-top text-center">
						<h1>One Account.</br> Everything Intuit</h1>
						<p>Create an account for all Intuit's products including Mint. <a href="#">Learn more</a></p>
			</div>
  					<!--Form-->
  			<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  			<div class="form-inputs">
    			<%= f.input :email, required: true, autofocus: true, %>
    			<%= f.input :password, required: false %>
    			<%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
			</div>
					<!--Bouton-->
  			<div class="form-actions">
   				 <%= f.button :submit, "Log in", class: "cols-sm-4 btn-big text-center" %>
			</div>
			<% end %>
		</div>
	</div>
</div>
